<% provide(:title, 'Lista użytkowników') %>
<br/><br/>
<table class="table table-bordered">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Actions</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= render :partial => "users/followForm", :locals => { :user => user } %></td>
  </tr>
<% end %>
</table>
